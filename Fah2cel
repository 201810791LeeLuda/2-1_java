import java.util.*;

public class Fah2Cel {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("화씨 온도를 입력하세요:");
        double f = sc.nextDouble();
        double c = (5 / 9.) * (f - 32);
        System.out.printf("Celsius temp: %.2f\n", c);
    }
}
