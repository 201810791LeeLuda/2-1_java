import java.util.Scanner;

public class Parc04_02 {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        System.out.println("정수를 입력하세요.");
        int n1=sc.nextInt();
        System.out.println("정수를 입력하세요.");
        int n2=sc.nextInt();
        if(n2%n1==0){
            System.out.printf("%d는 %d의 약수입니다.",n1,n2);
        }
        else{
            System.out.printf("%d는 %d의 약수가 아닙니다.",n1,n2);
        }
    }
}
